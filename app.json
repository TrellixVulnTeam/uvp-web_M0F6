{
    "name": "SeeVR",
    "repository": "https://github.com/garytyler/seevr-web",
    "addons": [
        "heroku-postgresql",
        "heroku-redis"
    ],
    "buildpacks": [
        {
            "url": "heroku/nodejs"
        },
        {
            "url": "heroku/python"
        }
    ],
    "env": {
        "ALLOWED_HOSTS": {
            "description": "The host/domain names that this Django site can serve, delimited by commas.",
            "value": ".herokuapp.com"
        },
        "SECRET_KEY": {
            "description": "A secret key for verifying the integrity of signed cookies.",
            "generator": "secret"
        }
    }
}